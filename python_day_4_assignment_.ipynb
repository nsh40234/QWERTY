      "source": [
        "n = int(input(\"Enter Any Number Of Your Choice: \"))\n",
        "def fact(n):\n",
        "    product = 1\n",
        "    for i in range(n):\n",
        "        product = product * (i+1)\n",
        "    return product\n",
        "\n",
        "f = fact(n)\n",
        "print(f)"
      ]
